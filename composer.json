{
    "name": "hyperf/rpc-multiplex",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "Rpc for multiplexing connection",
    "require": {
        "php": ">=8.0",
        "hyperf/codec": "~3.0.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/http-message": "~3.0.0",
        "hyperf/load-balancer": "~3.0.0",
        "hyperf/rpc": "~3.0.0",
        "hyperf/rpc-client": "~3.0.0",
        "hyperf/rpc-server": "~3.0.0",
        "hyperf/server": "~3.0.0",
        "hyperf/support": "~3.0.0",
        "hyperf/utils": "~3.0.0",
        "multiplex/socket": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\RpcMultiplex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\RpcMultiplex\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\RpcMultiplex\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
